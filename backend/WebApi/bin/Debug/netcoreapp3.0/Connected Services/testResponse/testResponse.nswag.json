  "paths": {
    "/api/quiz/QuestionsIds": {
      "get": {
        "tags": [
          "Ouiz"
        ],
        "summary": "Получить рандомные номера вопросов",
        "operationId": "QuestionsIds",
        "responses": {
          "200": {
            "description": "Возвращаем ответ",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Если 0",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
