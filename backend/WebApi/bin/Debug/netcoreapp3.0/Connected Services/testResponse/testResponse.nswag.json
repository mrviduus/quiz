        "operationId": "next",
        "parameters": [
          {
            "name": "questionId",
            "in": "query",
            "description": "id вопроса",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Возвращает Json",
